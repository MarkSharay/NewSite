@using PRAS_Task.Data.Models;
@model EngNew;

<form asp-action="Create" asp-controller="EngNews" enctype="multipart/form-data">
    <div>
        <div class="form-group">
            <label>Title </label>
            <input type="text" asp-for="name" class="form-control form-control-sm" />
        </div>
        <div class="form-group">
            <label> Photo</label>
            <input asp-for="picture" type="file" class="form-control  form-control-sm" />
        </div>
        <div class="form-group">
            <label>Text </label>
            <textarea asp-for="text" class="form-control  form-control-sm"></textarea>
        </div>
        <div class="form-group">
            <input class="btn btn-success btn-outline-dark" type="submit" value="Save" class="btn btn-success" />
        </div>
    </div>
</form>

<div>
    <button class="btn btn-primary btn-outline-dark">
        <a style="color: white; text-decoration: none;" asp-controller="EngNews" asp-action="Index">Back</a>
    </button>
</div>